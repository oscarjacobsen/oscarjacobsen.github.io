{
  "name": "trimble-connect-project-workspace-api",
  "version": "0.1.5",
  "description": "Trimble Connect for Web - Project Workspace API",
  "homepage": "http://connect.trimble.com",
  "files": [
    "dist"
  ],
  "main": "dist/cjs/trimble-connect.project.workspace.api.js",
  "module": "dist/es/trimble-connect.project.workspace.api.js",
  "extraModules": [
    "dist/iife/trimble-connect.project.workspace.api.js",
    "dist/amd/trimble-connect.project.workspace.api.js"
  ],
  "types": "dist/interfaces/index.d.ts",
  "scripts": {
    "build": "npm ci && rimraf dist/* && run-p build:*",
    "eslint-check": "eslint \"**/*.ts\"",
    "build:api": "cross-env BUILD_CONFIG=prod rollup -c",
    "build:typedef": "tsc -p tsconfig.typedef.json",
    "format": "eslint --fix \"**/*.ts\"",
    "deploy": "run-s deploy:*",
    "deploy:1": "cross-env-shell \"git tag -a \\\"$npm_package_name-v$npm_package_version\\\" -m $npm_package_commitName-v$npm_package_version\"",
    "deploy:2": "git push --follow-tags && npm publish"
  },
  "keywords": [
    "trimble",
    "trimble-connect",
    "trimble-connect-project-workspace-api",
    "tcweb",
    "api"
  ],
  "license": "https://community.trimble.com/docs/DOC-10021",
  "bugs": {
    "url": "https://jira.trimble.tools/browse/TWC"
  },
  "devDependencies": {
    "@fulcrum/lib": "^1.1.17",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.2.1",
    "cross-env": "^5.2.1",
    "eslint": "^7.32.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "rollup": "^2.3.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.0.3",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  }
}
